static_diagram Interfaces
--Shows the interfaces provided by the components 
component
	cluster core
	component
		class Console
			feature
				update 
					-> LIST[CHAR] 
				clear
		end	
		class ResourceHandler
			feature
				getRSLfiles: SET[FILE]
					->project: PROJECT
					require
						project /= Void
					end
				addFile 
					->file: FILE 
					->directory: DIRECTORY
					require
						file /= Void;
						directory /= Void
					ensure
						file member_of directory						
					end
				addProject 
					->directory: DIRECTORY 
					->project: PROJECT
					require
						directory /= Void;
						project /= Void
					ensure
						directory member_of project
					end
				delete 
					->file: FILE
					ensure 
						file = Void
					end
				deleteDir
					-> dir: DIR
					ensure 
						dir = Void
					end	
				deletePrj 
					->project: PROJECT
					ensure 
						project = Void
					end
		end	
		class TypeChecker
			feature
            	typeCheck: SET[ERROR]
                	->context: CONTEXT
                	require
                		context /= Void       
                	end
                getSignature: SIGNATURE
                	->entity: ENTITY
                	require
                		entity /= Void         
                	end
                getTheorems: SET[THEOREM]	
                	->context: CONTEXT  	
                	require
                		context /= Void
                	end
        end
		class RTestView
			feature
				update 
					-> restResults: SET[TESTRESULT]
		end
		
		class ProblemsView
			feature
				update 
					-> problems: SET[PROBLEM]
		end
		class REPLView
			feature
				clear
		end
		class OutlineView
			feature
				update 
					-> SIGNATURE
		end
		class ProofView
			feature
				update 
					-> SET[THEOREM]
		end
		class SMLTranslator
			feature
				translate 
					->context: CONTEXT
					require
						context /= Void
					end
		end
		
		class SMLCompiler
			feature
				compile: LIST[CHAR]
					->context: CONTEXT
					require
						context /= Void		
					end
		end
	end		
end